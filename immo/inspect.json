  "properties": {
    "item": {
      "type": "object",
      "properties": {
        "hashed_id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "image_url": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "quality": {
          "type": "string"
        },
        "vendor_price": {
          "type": "number"
        },
        "is_tradeable": {
          "type": "boolean"
        },
        "max_tier": {
          "type": "number"
        },
        "requirements": {
          "type": "object",
          "properties": {
            "combat": {
              "type": "number"
            },
            "defence": {
              "type": "number"
            }
          },
          "required": ["combat", "defence"]
        },
        "stats": {
          "type": "object",
          "properties": {
            "agility": {
              "type": "number"
            },
            "protection": {
