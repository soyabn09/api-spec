          "type": "object",
          "properties": {
            "agility": {
              "type": "number"
            },
            "protection": {
              "type": "number"
            }
          },
          "required": ["agility", "protection"]
        },
        "upgrade_requirements": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "item_id": {
                "type": "number"
              },
              "quantity": {
                "type": "number"
              }
            },
            "required": ["item_id", "quantity"]
          }
        },
        "health_restore": {},
        "hunger_restore": {}
      },
      "required": [
        "hashed_id",
        "name",
        "description",
        "image_url",
        "type",
        "quality",
        "vendor_price",
        "is_tradeable",
        "max_tier",
        "requirements",
        "stats",
        "effects",
        "tier_modifiers",
        "upgrade_requirements",
        "health_restore",
        "hunger_restore"
