{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Root",
  "type": "object",
  "properties": {
    "enemies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "image_url": {
            "type": "string"
          },
          "level": {
            "type": "number"
          },
          "experience": {
            "type": "number"
          },
          "health": {
            "type": "number"
          },
          "chance_of_loot": {
            "type": "number"
          },
          "location": {
            "type": "object",
            "properties": {
              "id": {
                "type": "number"
              },
              "name": {
                "type": "string"
              }
            },
            "required": ["id", "name"]
          },
          "loot": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "hashed_item_id": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "image_url": {
                  "type": "string"
                },
                "quality": {
                  "type": "string"
                },
                "quantity": {
                  "type": "number"
                },
                "chance": {
                  "type": "number"
                }
              },
              "required": [
                "hashed_item_id",
                "name",
                "image_url",
                "quality",
                "quantity",
                "chance"
              ]
            }
          }
        },
        "required": [
          "id",
          "name",
          "image_url",
          "level",
          "experience",
          "health",
          "chance_of_loot",
          "location",
          "loot"
        ]
      }
    },
    "endpoint_updates_at": {
      "type": "string"
    }
  },
  "required": ["enemies", "endpoint_updates_at"]
}
